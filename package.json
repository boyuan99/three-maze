{
  "name": "three-maze",
  "version": "0.0.2",
  "description": "A VR environment designed for animal behavior research.",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "cross-env NODE_ENV=development concurrently -k \"vite\" \"wait-on tcp:5173 && electron .\"",
    "electron:build": "vite build && electron-builder",
    "postinstall": "node setup/setup-python.js"
  },
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.14.0",
    "cross-env": "^7.0.3",
    "three": "^0.169.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "concurrently": "^9.0.1",
    "electron": "^33.0.1",
    "electron-builder": "^25.1.8",
    "vite": "^5.4.9",
    "wait-on": "^8.0.1"
  },
  "build": {
    "appId": "com.example.three-maze",
    "productName": "three-maze",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "mac": {
      "target": ["dmg"]
    },
    "asar": false
  }
}
